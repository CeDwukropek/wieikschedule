
# Optional: metadata collection that lists available schedules.
# Each doc should contain at least { collectionId, name }.
REACT_APP_FIREBASE_SCHEDULE_INDEX_COLLECTION=schedules

# Optional: comma-separated Firestore collection names.
# Each collection is treated as one selectable schedule in the app.
# Example: AwP4.0s1,AwP4.0s2
REACT_APP_FIREBASE_SCHEDULE_COLLECTIONS=EEs1, EEs3

# Legacy fallback (only if you use old one-collection timetable documents)
REACT_APP_FIREBASE_TIMETABLES_COLLECTION=timetables
